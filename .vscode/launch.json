{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Main",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "godot",
            "args": [
                "--path",
                "${workspaceFolder}"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch Scene",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "godot",
            "args": [
                "--path",
                "${workspaceFolder}",
                "${command:godot.csharp.getLaunchScene}"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}
